class Node():

    # Constructor method
    def __init__(self, item_data):
        self.data = item_data
        self.next = None

    def get_data(self):
        return self.data

    def get_priority(self):
        return self.priority

    def get_next(self):
        return self.next

    def set_next(self, new_next):
        self.next = new_next   


class PriorityStack():

    # Constructor method
    def __init__(self):
        self.top = None
        self.rear = None

    def get_top(self):
        return self.top

    def set_top(self, new_top):
        self.top = new_top

    def isempty(self):
        if self.top == None:
            return True
        else:
            return False
    
    def enstack(self,data):

        newnode = Node(data)

        if self.isempty():

            # if list is empty then newnode will be front 
            self.set_top(newnode)
        
        else:
            
            newnode.set_next(self.get_top())
            self.set_top(newnode)
            


    def destack(self):

        removed = None

        if self.isempty():
            print("cant deque an empty list")
        
        else:
            self.set_top(self.top.get_next())
        
        return removed


list2 = PriorityStack()

list2.enstack("josh")
list2.enstack("tom")
list2.enstack("amey")
list2.enstack("alex")
list2.destack()
print(list2)
